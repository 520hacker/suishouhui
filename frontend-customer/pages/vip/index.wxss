/* pages/vip/index.wxss */
.memberinfo {
  text-align: center;
  width: 33%;
  border-right: solid gray 1px;
}

.memberinfo_coupon {
  text-align: center;
  width: 34%;
}

.member-tip {
  color: gray;
  text-align: center;
  font-size: 13px;
}

.member-number {
  text-align: center;
  font-size: 15px;
  color:#B22222;
}

.memberinfo-avatar {
  width: 110rpx;
  height: 110rpx;
  display: block;
  overflow: hidden;
  border-radius: 50%;
}

.memberinfo-name {
  float: left;
  margin-left: 10px;
  font-size: 15px;
}
.switch {
  margin-left: 5px;
}

.memberinfo-grade {
  margin-left: 10px;
  font-size: 13px;
}

.member-background {
  padding-top: 15px;
  color: black;
  border-radius: 5px;
}

.account-box {
  padding-top: 20px;
  padding-bottom: 10px;
}
.weui-cells {
  font-size: 12px;
  margin-top:5px;
  margin-left:2%;
  margin-right:2%;
}

.wei-cell {
  padding: 5px 12px;
}
.card-qrcode{
  width: 25px;
  height: 25px;
}
.clear {
  clear:both;
}
.footer {
  margin-top: 30px;
  margin-bottom: 10px;
  width: 100%;
  text-align: center;
  font-size: 12px;
  color: gray;
  position: fixed;
  bottom: 0px;
}